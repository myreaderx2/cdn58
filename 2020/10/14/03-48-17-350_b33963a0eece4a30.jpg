<div id="currency-panel" style="display:none" class="panel-mobile">
  <!-- currency Start -->
   <div class="ng-scope">
    <div class="section-header currency-header">
      <a class="left-panel-button menu-button ie-hack " href="#left-panel">
        <span class="fa fa-chevron-left"></span>
      </a>
      Currency
    </div>
  </div>
  <ul>
